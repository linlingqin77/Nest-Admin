{
  "name": "Nest-Admin-Soybean",
  "type": "module",
  "version": "1.2.1",
  "description": "基于 Vue3 + NestJS 的现代化企业级后台管理系统，集成了强大的权限管理和丰富的组件库。",
  "license": "MIT",
  "homepage": "https://github.com/linlingqin77/Nest-Admin-Soybean",
  "keywords": [
    "Nest-Admin-Soybean",
    "Vue3",
    "NestJS",
    "admin-template",
    "Vite7",
    "TypeScript",
    "naive-ui",
    "naive-ui-admin",
    "UnoCSS"
  ],
  "contributors": ["linlingqin77"],
  "engines": {
    "node": ">=20.19.0",
    "pnpm": ">=10.5.0"
  },
  "scripts": {
    "build": "vite build --mode prod",
    "build:dev": "vite build --mode dev",
    "build:test": "vite build --mode test",
    "cleanup": "sa cleanup",
    "commit": "sa git-commit",
    "commit:zh": "sa git-commit -l=zh-cn",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "deploy:dev": "node scripts/deploy.cjs dev",
    "deploy:prod": "node scripts/deploy.cjs prod",
    "deploy:test": "node scripts/deploy.cjs test",
    "dev": "vite --mode dev",
    "dev:prod": "vite --mode prod",
    "dev:test": "vite --mode test",
    "format": "prettier --write .",
    "gen-route": "sa gen-route",
    "gen:api": "tsx scripts/post-gen-api.ts",
    "lint": "eslint . --fix",
    "prepare": "simple-git-hooks",
    "preview": "vite preview",
    "release": "sa release",
    "test": "vitest run",
    "test:all": "vitest run && cypress run",
    "test:cov": "vitest run --coverage",
    "test:e2e": "cypress run",
    "test:ui": "vitest --ui",
    "test:watch": "vitest",
    "typecheck": "vue-tsc --noEmit --skipLibCheck",
    "update-pkg": "sa update-pkg"
  },
  "dependencies": {
    "@better-scroll/core": "2.5.1",
    "@iconify/vue": "5.0.0",
    "@sa/axios": "workspace:*",
    "@sa/color": "workspace:*",
    "@sa/hooks": "workspace:*",
    "@sa/materials": "workspace:*",
    "@sa/tinymce": "workspace:*",
    "@sa/utils": "workspace:*",
    "@types/streamsaver": "^2.0.5",
    "@umoteam/editor": "^8.1.0",
    "@vueuse/core": "14.0.0",
    "clipboard": "2.0.11",
    "cron-parser": "^5.4.0",
    "dayjs": "1.11.19",
    "defu": "6.1.4",
    "echarts": "6.0.0",
    "element-plus": "^2.12.0",
    "highlight.js": "^11.11.1",
    "jsencrypt": "^3.3.2",
    "json5": "2.2.3",
    "monaco-editor": "^0.52.2",
    "naive-ui": "2.43.1",
    "nprogress": "0.2.0",
    "pinia": "3.0.4",
    "streamsaver": "^2.0.6",
    "tailwind-merge": "3.4.0",
    "vform3-builds": "^3.0.10",
    "vue": "3.5.24",
    "vue-advanced-cropper": "^2.8.9",
    "vue-draggable-plus": "0.6.0",
    "vue-i18n": "11.1.12",
    "vue-router": "4.6.3",
    "xlsx": "0.18.5"
  },
  "devDependencies": {
    "@elegant-router/vue": "0.3.8",
    "@hey-api/openapi-ts": "^0.90.2",
    "@iconify/json": "2.2.407",
    "@iconify/utils": "^2.1.33",
    "@sa/scripts": "workspace:*",
    "@sa/uno-preset": "workspace:*",
    "@soybeanjs/eslint-config": "1.7.3",
    "@types/node": "24.10.1",
    "@types/nprogress": "0.2.3",
    "@unocss/eslint-config": "66.5.6",
    "@unocss/preset-icons": "66.5.6",
    "@unocss/preset-uno": "66.5.6",
    "@unocss/transformer-directives": "66.5.6",
    "@unocss/transformer-variant-group": "66.5.6",
    "@unocss/vite": "66.5.6",
    "@vitejs/plugin-vue": "6.0.1",
    "@vitejs/plugin-vue-jsx": "5.1.1",
    "@vitest/coverage-v8": "^4.0.16",
    "@vue/test-utils": "^2.4.6",
    "chalk": "^4.1.2",
    "compressing": "^1.10.0",
    "consola": "3.4.2",
    "cypress": "^15.8.2",
    "eslint": "9.39.1",
    "eslint-plugin-vue": "10.5.1",
    "fast-check": "^4.5.3",
    "jsdom": "^27.4.0",
    "kolorist": "1.8.0",
    "msw": "^2.12.7",
    "ora": "^5.4.1",
    "prettier": "^3.3.3",
    "sass": "1.94.0",
    "simple-git-hooks": "2.13.1",
    "ssh2": "^1.15.0",
    "tsx": "4.20.6",
    "typescript": "5.9.3",
    "unplugin-icons": "22.5.0",
    "unplugin-vue-components": "30.0.0",
    "vite": "7.2.2",
    "vite-plugin-monaco-editor": "^1.1.0",
    "vite-plugin-progress": "0.0.7",
    "vite-plugin-static-copy": "^3.1.0",
    "vite-plugin-svg-icons": "2.0.1",
    "vite-plugin-vue-devtools": "8.0.3",
    "vitest": "^4.0.16",
    "vue-eslint-parser": "10.2.0",
    "vue-tsc": "3.1.4"
  },
  "simple-git-hooks": {
    "commit-msg": "pnpm sa git-commit-verify && commitlint --edit $1",
    "pre-commit": "pnpm typecheck && pnpm lint && git diff --exit-code"
  },
  "website": "https://ruoyi.xlsea.cn"
}
